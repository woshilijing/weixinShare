var nav_link = {'home':'home/','news':'news/','social':'social/','finacial':'finacial/','public':'public/','entertainment':'entertainment/','film':'film/','tv':'tv/','meeting':'meeting/','life':'life/','sports':'sports/'};

    function slide_nav(obj,top){
        if (!obj)
        {
            _('.slide_2015').attr({'status':'close'}).animate({'top':'-200px'},300);
            _('.fullbg').fadeOut(300,function(){
                _('.fullbg').hide().height('100%');
            });
            return;
        }
        if (_(obj).attr('status') != 'open')
        {
            _('.slide_2015').css({'top':'-200px'});
            _(obj).attr({'status':'open'}).animate({'top':'47px'},300);
            _('.fullbg').css({'opacity':0}).show().animate({'opacity':0.3},300).refillscreen();
        }else{
            _(obj).attr({'status':'close'}).animate({'top':'-200px'},300);
            _('.fullbg').fadeOut(300).height('100%');
        }
    }
_(window).resize(function(){
    _('.fullbg').refillscreen();
    _('#ynetVideoIframe').css({'width':'100%','height':(_('#ynetVideoIframe').parent().width()*9/16)+'px'});
});
var load_pics = 0,load_recommend = 0,inited_recommend = false,more_rdata = [],inlist_no = 0,timer=null;

function getmore(force){
    if ((_.pageSize().pageHeight > _.pageSize().windowHeight + _.scrollSize().top) && !force){
        return ;
    }
    wrap_recommend();
}
_(window).bind('scroll',function(){
    getmore(false);
});

function wrap_pics(){
    _.ajax({
        'type':'GET',
        'data':{},
        'url':(_('#static_url').attr('content')?_('#static_url').attr('content'):'http://res.ynet.com/')+"YnetView/rttj.json",
        'fnTimeout':function(){
            if (!load_pics)
            {
                load_pics = 1;
                wrap_pics();
            }
        },
        'success':function(result){
            if (result)
            {
                var str = '';
                for (var i in result )
                {
                    var data = result[i];
                    str += '<div class="tuji_list'+(i%2?' tuji_right':' tuji')+'"><a href="'+data['link']+'">'+data['imgs']+'</a><p><a href="'+data['link']+'">'+data['title']+'</a></p></div><div class="fgs"></div>';
                    if (i > 6)
                    {
                        break;
                    }
                }
                _('#wrap_pics').html(str);
                var tmp = 0;
                _('.tuji_list').each(function(){
                    tmp += 1;
                    if(tmp > 6){
                        _(this).addClass('tuji_short');
                    }
                });
            }
            wrap_recommend();
        }
    });
}

function more_recommend(){
    var total = more_rdata.length;
    var this_no = 0,tmp_no = inlist_no;
    for (var i=tmp_no;i<total ;i++ )
    {
        var tmp = document.createElement('div');
        _(tmp).addClass('tuijian').html(more_rdata[i]);
        _('#wrap_recommend_bottom').append(tmp);
        inlist_no += 1;
        this_no += 1;
        if (this_no == 5)
        {
            break;
        }
    }
    _('#end').css({'height':'2rem !important'});
    _('#jz_bottom').hide();
}

