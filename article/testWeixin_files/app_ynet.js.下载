                        };
                    }
                    inited_recommend = true;
                }
            }
        });
    }else{
        if (!more_rdata.length || inlist_no == more_rdata.length)
        {
            return ;
        }
        _('#end').css({'height':'auto !important'});
        if (!_('#jz_bottom').size()) {
            _('#end').html('<div id="jz_bottom">加载更多</div>'+_('#end').html());
        };
        _('#jz_bottom').show();
        window.clearTimeout(timer);
        timer = setTimeout(function(){more_recommend();},1200);
    }
}
_.ready(function(){
    _('#ynetVideoIframe').css({'width':'100%','height':(_('#ynetVideoIframe').parent().width()*9/16)+'px'});
    var top_back_url = '';
    var referer = document.referrer;
	top_back_url = 'http://'+document.domain;
    if (!referer || referer.indexOf(document.domain) == -1)
    {
	top_back_url = 'http://www.ynet.com';
    }
    if (referer && referer.indexOf(document.domain) != -1)
    {
        var tmp = referer.split('/');
	top_back_url = 'http://'+tmp[2]+'/';
    }
        if (top_back_url){
      _('.top_back').click(function(){
        location.href = top_back_url;
      });
        }
        _('#gentie').prev().css({'width':'100%'});
    _('.nav_2015 a').each(function(){
        if (_(this).attr('data') && nav_link[_(this).attr('data')])
        {
            _(this).attr('href',nav_link[_(this).attr('data')]);
        }
    });
    _('.share_sina').click(function(){
     window.open('http://v.t.sina.com.cn/share/share.php?title=' + encodeURIComponent(document.title+'±±ÇàÍø'+location.href+')') + '&pic=&sourceUrl='+encodeURIComponent(location.href)+ ',_parent,toolbar=0,status=0,resizable=1,width=100%,height=500,left=0,top='+(_(window).height-500)/2);
    });
    _('.nav_icon').click(function(event){
        _.stopevent(event);
        slide_nav('.nav_2015','-15.8rem');
        return ;
    });
    _('.share_icon').click(function(event){
        _.stopevent(event);
        slide_nav('.share_2015','-10.8rem');
        return ;
    });
    _('#searchipt').click(function(event){
        _.stopevent(event);
        if (_(this).attr('status') == 'max')
        {
            return ;
        }
        _(document.body).css({'padding-bottom':'8.5rem'});
        _(this).attr('status','max');
        _('.share_btn').hide();
        _('#pinglun').css({'height':'20rem'});
        _('#pinglun .pl').css({'margin-top':'5px'});
        _('#searchipt').addClass('comment_text_close');
        _('#fx').hide();
        _('.btn_comment').show();
    });
    _('#searchipt').keyup(function(){
        _('.area-text-wap').val(_(this).val());
    });
    _('.btn_comment').click(function(){
        _('.mutual-btn-wap').click();
    });
    _('.fullbg,#logo').click(function(event){
        _.stopevent(event);
        slide_nav(false);
        return ;
    });
    _(document.body).click(function(event){
            if (_('#searchipt').attr('status') == 'min')
            {
                return ;
            }
            _(document.body).css({'padding-bottom':'0px'});
            _('#searchipt').attr('status','min');
            _('.btn_comment').hide();
            _('#pinglun').css({'height':'100px'});
            _('#searchipt').removeClass('comment_text_close');
            //_('#searchipt').css({'height':'40px','max-width':'50%','width':'50%','float':'left','margin':'30px','padding':'0px 2%'});
            _('#fx').hide();
            _('.share_btn').fadeIn();
    });
        
    wrap_pics();
});

//for weixin
(function() {
    var ua = window.navigator.userAgent.toLowerCase();
    if(ua.match(/MicroMessenger/i) == 'micromessenger'){
        var sc = document.createElement("script");
        sc.charset="utf-8";
        sc.src = "//wxif.ynet.com/app.min.js";
        var s = document.getElementsByTagName("script")[0]; 
        s.parentNode.insertBefore(sc, s);
    }
})();